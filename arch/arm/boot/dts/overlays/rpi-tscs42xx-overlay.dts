// Definitions for Tempo Semiconductor tscs42xx 
/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2708";

	fragment@0 {
		target = <&i2c1>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			codec: tscs42A2@69 {
				compatible = "tempo,tscs42A2";
				reg = <0x69>;
				status = "okay";
			};
		};
	};

	fragment@1 {
		target = <&i2s>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@2 {
		target = <&sound>;
		__overlay__ {
			compatible = "tempo,rpi-wookie";
			i2s-controller = <&i2s>;
			audio-codec = <&codec>;
			mclk-src = "xtal";	
			mclk-src-freq = <12288000>;
			audio-routing =
				"Headphone Jack", "Headphone L",
				"Headphone Jack", "Headphone R",
				"Speaker", "Speaker L",
				"Speaker", "Speaker R",
				"Line In 3 L", "Digital Mic",
				"Line In 3 R", "Digital Mic",
				"Digital Mic", "Digital Mic Enable",
				"Line In 1 L", "Analog Mic",
				"Line In 1 R", "Analog Mic",
				"Analog Mic", "Mic Bias",
				"Line In 2 L", "Line In",
				"Line In 2 R", "Line In";
			status = "okay";
			hp-gpios = <&gpio 16 1>; //<GPIO_ACTIVE_LOW>;
		//	mic-gpios = <&gpio 17 0>; //<GPIO_ACTIVE_HIGH>;
		};
	};
};
